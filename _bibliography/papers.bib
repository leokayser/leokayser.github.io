@Article{BK2024mukailifting,
  selected={true},
  abbr={Article},
  title={Mukai lifting of self-dual points in ℙ⁶}, 
  abstract={A set of 2n points in ℙ^{n-1} is self-dual if it is invariant under the Gale transform.
  Motivated by Mukai's work on canonical curves, Petrakiev showed that a general self-dual set of 14 points in ℙ⁶ arises as the intersection of the Grassmannian Gr(2,6) in its Plücker embedding in ℙ¹⁴ with a linear space of dimension 6.
  In this paper we focus on the inverse problem of recovering such a linear space associated to a general self-dual set of points.
  We use numerical homotopy continuation to approach the problem and implement an algorithm in Julia to solve it. Along the way we also implement the forward problem of slicing Grassmannians and use it to experimentally study the real solutions to this problem.},
  author={Barbara Betti and Leonie Kayser},
  year={2024},
  month={June},
