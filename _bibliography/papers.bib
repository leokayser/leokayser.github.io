@Article{GKT2023choppedideals,
  selected={true},
  abbr={Article},
  title={Hilbert Functions of Chopped Ideals}, 
  abstract={A chopped ideal is obtained from a homogeneous ideal by considering only the generators of a fixed degree.
    We investigate cases in which the chopped ideal defines the same finite set of points as the original one-dimensional ideal.
    The complexity of computing these points from the chopped ideal is governed by the Hilbert function and regularity.
    We conjecture values for these invariants and prove them in many cases.
    We show that our conjecture is of practical relevance for symmetric tensor decomposition.},
